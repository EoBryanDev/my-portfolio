{
    //"parser": "babel-eslint",
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "plugins": [
      "react","jsx-a11y", "import"
    ],
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "rules": {
      "react/jsx-filename-extension": [
        "error",
        {
          "extensions": [".js", ".jsx"]
        }
      ],
      "global-require": "off",
      "import/prefer-default-export": "off",
      "no-unused-expressions": ["error", { "allowTaggedTemplates": true }],
      "react/function-component-definition": "off"
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    }

}
